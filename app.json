{
  "expo": {
    "name": "CarRentalRNExpo",
    "slug": "car-rental-ern",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cars.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash_full.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.CarRentalRNExpo"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cars.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.CarRentalRNExpo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/cars.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      "@react-native-google-signin/google-signin",
      "expo-router",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Apakah boleh $(PRODUCT_NAME) menggunakan lokasi Anda?"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Apakah boleh $(PRODUCT_NAME) menggunakan file gambar Anda?",
          "cameraPermission": "Apakah boleh $(PRODUCT_NAME) menggunakan kamera Anda?"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f80a325a-8c3e-4572-a67e-28356542439e"
      }
    },
    "owner": "erdiys"
  }
}
